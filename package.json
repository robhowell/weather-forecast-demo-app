{
  "name": "weather-forecast-demo-app",
  "version": "1.0.0",
  "description": "5-day weather forecast using the openweatherapp.com API",
  "main": "index.js",
  "scripts": {
    "start": "serve dist --port 80",
    "build": "webpack --mode=production",
    "now-build": "webpack --mode=production",
    "dev": "webpack-dev-server --open",
    "test": "jest",
    "lint": "eslint src",
    "update-dependencies": "npx npm-check-updates --upgrade && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robhowell/weather-forecast-demo-app.git"
  },
  "keywords": [
    "weather",
    "forecast",
    "react",
    "webpack"
  ],
  "author": "Rob Howell <rob@robhowell.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robhowell/weather-forecast-demo-app/issues"
  },
  "homepage": "https://github.com/robhowell/weather-forecast-demo-app#readme",
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-react-require": "3.1.1",
    "css-loader": "3.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.15.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.8.0",
    "node-sass": "4.12.0",
    "react-test-renderer": "16.9.0",
    "sass-loader": "7.2.0",
    "style-loader": "1.0.0",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "serve": "^11.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ]
  }
}
